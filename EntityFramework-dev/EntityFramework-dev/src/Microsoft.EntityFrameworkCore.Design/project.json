{
  "version": "1.2.0-*",
  "description": "Shared design-time components for Entity Framework Core tools.",
  "packOptions": {
    "tags": [
      "Entity Framework Core",
      "entity-framework-core",
      "EF",
      "Data",
      "O/RM"
    ]
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true,
    "compile": {
      "include": "../Shared/*.cs"
    }
  },
  "dependencies": {
    "Microsoft.AspNetCore.Hosting.Abstractions": "1.2.0-*",
    "Microsoft.EntityFrameworkCore.Relational.Design": "1.2.0-*",
    "NETStandard.Library": "1.6.1-*"
  },
  "frameworks": {
    "net451": {},
    "netstandard1.3": {
      "dependencies": {
        "System.Collections.NonGeneric": "4.3.0-*"
      }
    }
  }
}